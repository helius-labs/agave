                    "rawSql": "SELECT\n  $__timeInterval(sl.timestamp) AS time,\n  concat(coalesce(vm.name, ''), ' (', toString(sl.data.leader_sent), ')') AS leader,\n  avg(toUInt64(sl.data.latency_ns)) AS avg_latency\nFROM clickhouse_logs.slot_latency sl\nLEFT JOIN (\n  SELECT \n    identity,\n    argMax(data.name.:String, timestamp) as name\n  FROM clickhouse_logs.validator_metadata\n  GROUP BY identity\n) vm ON toString(sl.data.leader_sent) = vm.identity\nWHERE\n  $__timeFilter(sl.timestamp)\n  AND sl.data.leader_sent IS NOT NULL\n  AND sl.data.slot_sent IS NOT NULL\n  AND sl.data.slot_landed IS NOT NULL\n  AND toUInt64(sl.data.slot_sent) = toUInt64(sl.data.slot_landed)\n  AND sl.data.latency_ns IS NOT NULL\n  AND sl.data.latency_ns > 0\n  AND sl.data.latency_ns < 300000000000\n  AND match(toString(sl.data.host), '${host:regex}')\n  AND match(toString(sl.data.staked), '${staked:regex}')\n  AND match(toString(sl.data.is_relayed), '${is_relayed:regex}')\n  AND (${priority} = 0 OR toUInt64(sl.data.priority) >= ${priority})\n  AND ('${leader_search}' = '' OR toString(sl.data.leader_sent) LIKE '%${leader_search}%')\nGROUP BY time, leader\nORDER BY time",
